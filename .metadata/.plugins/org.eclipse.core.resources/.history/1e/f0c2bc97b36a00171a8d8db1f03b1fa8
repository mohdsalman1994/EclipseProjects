package com.aceprogrammer.basics.CliniqueManagement;

public class Doctor {
	
	private int id;
	private String name;
	private String specialization;
	private Availibility availibility;
	public Doctor(int id, String name, String specialization, Availibility availibility) {
		super();
		this.id = id;
		this.name = name;
		this.specialization = specialization;
		this.availibility = availibility;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSpecialization() {
		return specialization;
	}
	public void setSpecialization(String specialization) {
		this.specialization = specialization;
	}
	public Availibility getAvailibility() {
		return availibility;
	}
	public void setAvailibility(Availibility availibility) {
		this.availibility = availibility;
	}
	
	
	

}
